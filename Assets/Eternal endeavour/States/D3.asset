    \u043D\u0435 \u0443\u0432\u0435\u0440\u0435\u043D\u0430, \u0441\u043A\u043E\u043B\u044C\u043A\u043E
    \u0435\u0449\u0435 \u043F\u0440\u043E\u0434\u0435\u0440\u0436\u0430\u0442\u0441\u044F
    \u0442\u0432\u043E\u0438 \u043D\u043E\u0433\u0438, \u0438, \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E,
    \u0442\u0430\u043C \u043A\u0442\u043E-\u0442\u043E \u0441\u043C\u043E\u0436\u0435\u0442
    \u043F\u043E\u043C\u043E\u0447\u044C."
